{
  "name": "Onay",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "http://www.onay.kz/*",
    "http://fonts.googleapis.com/",
    "https://fonts.googleapis.com/"
  ],
  "browser_action": {
    "default_icon": {
      "38": "img/icon-38.png"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  }
}